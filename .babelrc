{
  "presets": [
    ["env", {
      "module": false,
      "targets": {
        "node": "current"
      }
    }],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ],
  "ignore": [
    "node_modules/**/*.js",
    "server/bundles/**/*.js",
    "server/bin/**/*.js"
  ]
}
