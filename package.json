  "scripts": {
    "dev": "node bin/server.js ",
    "build-client": "webpack --progress --display-error-details --bail",
    "build-server": "babel server --out-dir bundle-server",
    "start": "forever bundle-server/server.js",
    "clear-bundles": "rimraf bundle-server && rimraf static/bundles",
    "clean": "npm prune --production",
    "build-app": "npm run clear-bundles && npm run build-server && npm run build-client",
    "heroku-postbuild": "npm run clear-bundles && npm run build-app"
  },
  "dependencies": {
    "forever": "^0.15.3",
    "history": "^4.6.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
