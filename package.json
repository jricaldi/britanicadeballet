    "html-webpack-plugin": "^2.28.0",
    "imagemin-webpack-plugin": "^1.4.4",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "postcss-smart-import": "^0.6.10",
    "precss": "^1.4.0",
    "react-hot-loader": "next",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
